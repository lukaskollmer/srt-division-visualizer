{
    "compilerOptions": {
        "module": "es6",
        "target": "es2016",
        "jsx": "preserve",
        "outDir": "build",
        "noImplicitReturns": true,
        "noImplicitAny": true
    },
    "include": [
        "srt/*",
        "app/*"
    ],
    "exclude": [
        "node_modules/*"
    ],
    "typesRoot": [
        "node_modules/@types"
    ]
}
